{
  "functions": {
    "api/*": {
      "runtime": "vercel-php@0.7.1"
    },
    "/api/public/*": {
      "runtime": "@vercel/static"
    }
  },
  "routes": [
    {
      "src": "/(css)/(.*)",
      "dest": "api/public/$1/$2"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}